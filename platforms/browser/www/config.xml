<?xml version='1.0' encoding='utf-8'?>
<widget id="net.gaelmrt.gamedb" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GameDb</name>
    <description>
        An application to see all the games that will come out soon or that went out recently (one week).
    </description>
    <author email="gael.mrt@eduge.ch" href="http://cordova.io">
        Gael MRT
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon height="1024" src="res/icons/Icon-1024_opaque.png" width="1024" />
</widget>
